{
    "name": "autospecai",
    "version": "0.0.16",
    "description": "Open-source AI agent that autonomously QAs and generates E2E test specs for your web app",
    "bin": {
        "autospecai": "build/src/cli.js"
    },
    "type": "module",
    "types": "build/src/index.d.ts",
    "main": "build/src/index.js",
    "keywords": [
        "ai",
        "e2e",
        "testing",
        "test",
        "e2e",
        "end-to-end",
        "playwright",
        "gpt"
    ],
    "author": "Zach Blume <zachblume@gmail.com>",
    "license": "MIT",
    "scripts": {
        "lint": "eslint . --fix",
        "format": "npx prettier --write .",
        "test": "npx vitest run ./unittests",
        "build": "tsc -p ./tsconfig.json",
        "prebuild": "rm -rf ./build",
        "benchmark": "node ./build/benchmark/benchmark.js",
        "tsc": "tsc -p ./tsconfig.json --noEmit"
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx,json,css,scss}": "prettier --write"
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^0.0.30",
        "@ai-sdk/google": "0.0.26",
        "@ai-sdk/openai": "^0.0.36",
        "@inquirer/prompts": "^5.1.2",
        "ai": "^3.2.22",
        "chalk": "^5.3.0",
        "dotenv": "^16.4.5",
        "playwright": "^1.45.1",
        "sharp": "^0.33.4",
        "strip-ansi": "^7.1.0",
        "winston": "^3.13.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.24.8",
        "@eslint/js": "^9.7.0",
        "@playwright/test": "^1.45.1",
        "@types/inquirer": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^7.16.0",
        "@typescript-eslint/parser": "^7.16.0",
        "babel-jest": "^29.7.0",
        "eslint": "^8.57.0",
        "globals": "^15.8.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.7",
        "prettier": "^3.3.3",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.3.0",
        "vitest": "^2.0.2"
    }
}
