{
    "name": "autospecai",
    "version": "0.0.3",
    "description": "Open-source AI agent that autonomously QAs and generates E2E test specs for your web app",
    "bin": {
        "autospecai": "cli.js"
    },
    "type": "module",
    "keywords": [
        "ai",
        "e2e",
        "testing",
        "test",
        "e2e",
        "end-to-end",
        "playwright",
        "gpt"
    ],
    "author": "Zach Blume <zachblume@gmail.com>",
    "license": "MIT",
    "scripts": {
        "lint": "eslint . --fix",
        "format": "npx prettier --write ."
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx,json,css,scss}": "prettier --write"
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^0.0.19",
        "@ai-sdk/google": "^0.0.20",
        "@ai-sdk/openai": "^0.0.24",
        "@playwright/test": "^1.44.1",
        "ai": "^3.1.30",
        "canvas": "^2.11.2",
        "chalk": "^5.3.0",
        "dotenv": "^16.4.5",
        "fluent-ffmpeg": "^2.1.3",
        "playwright": "^1.44.1",
        "strip-ansi": "^7.1.0",
        "winston": "^3.13.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "eslint": "^9.4.0",
        "globals": "^15.4.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "jest-esm-transformer": "^1.0.0",
        "lint-staged": "^15.2.5",
        "prettier": "^3.3.1"
    }
}
